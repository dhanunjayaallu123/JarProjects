pipeline{
agent any 
tools{
maven 'apache-maven-3.9.0'
}
stages{
stage('clone url'){
steps{
git branch: 'master', url: 'https://github.com/dhanunjayaallu123/JarProjects.git'
}
}
stage('buil with maven'){
steps{
 bat '''mvn clean'''
}
}
stage('deploy to tomcat'){
steps{
echo 'deploy to tomcat container'
}
}
}
}
